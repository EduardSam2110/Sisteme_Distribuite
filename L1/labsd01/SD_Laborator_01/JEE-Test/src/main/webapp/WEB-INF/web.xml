<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>Hello</servlet-name>
    <servlet-class>HelloServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ProcessStudent</servlet-name>
    <servlet-class>ProcessStudentServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ReadStudent</servlet-name>
    <servlet-class>ReadStudentServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>InitDatabase</servlet-name>
    <servlet-class>InitDatabaseServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ProcessStudentDB</servlet-name>
    <servlet-class>ProcessStudentDatabaseServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ReadStudentDB</servlet-name>
    <servlet-class>ReadStudentDatabaseServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>Hello</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ProcessStudent</servlet-name>
    <url-pattern>/process-student</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ReadStudent</servlet-name>
    <url-pattern>/read-student</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>InitDatabase</servlet-name>
    <url-pattern>/init-database</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ProcessStudentDB</servlet-name>
    <url-pattern>/process-student-db</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ReadStudentDB</servlet-name>
    <url-pattern>/read-student-db</url-pattern>
  </servlet-mapping>
</web-app>
